	0 -> 1 [label=a]
	2 -> 3 [label=b]
	1 -> 2 [label="ε"]
	4 -> 5 [label=b]
	5 -> 4 [label="ε"]
	5 -> 7 [label="ε"]
	6 -> 4 [label="ε"]
	3 -> 6 [label="ε"]
}
